{
  "name": "infrastructure-logging-lib",
  "repository": {
    "type": "git",
    "url": "git@github.com:Placester/Infrastructure-Logging-Lib.git"
  },
  "version": "2.1.1",
  "description": "",
  "main": "lib/index.js",
  "types": "dts/index.d.ts",
  "scripts": {
    "posttest": "NODE_ENV=test istanbul check-coverage && node_modules/.bin/standard test/*/*.js && npm run package",
    "pretest": "node_modules/.bin/tsc -p ./",
    "package": "rm -rf dist && rm -rf dist && rm -rf lib && tsc -p ./ --declaration --outDir lib",
    "test": "NODE_ENV=test istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/unit/*",
    "sync": "aws s3 sync ./config s3://etl-code-config/infrastructure-logging-lib/$npm_package_version/config"
  },
  "config": {
    "coverage": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.7",
    "bunyan": "^1.8.5",
    "debug": "^2.6.0",
    "kinesis": "^1.2.2",
    "le_node": "^1.7.0",
    "lodash": "^4.17.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/bunyan": "0.0.35",
    "@types/config": "0.0.31",
    "@types/lodash": "^4.14.52",
    "@types/node": "^7.0.5",
    "chai": "^3.5.0",
    "config": "^1.25.1",
    "faker": "^3.1.0",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "standard": "^8.6.0",
    "tslint-config-standard": "^3.0.0",
    "typescript": "^2.1.5"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    }
  }
}
