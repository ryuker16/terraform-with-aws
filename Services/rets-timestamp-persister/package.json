{
  "name": "rets-timestamp-persister",
  "version": "0.0.1",
  "description": "Persists RETS incremental timestamps from the extractor to mongodb",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test DEBUG=logginglib gulp test",
    "test-coverage": "NODE_ENV=test DEBUG=logginglib gulp test-coverage",
    "preitest": "gulp clean && gulp build-src build-test",
    "itest": "NODE_ENV=test DEBUG=logginglib mocha dist/test/integration",
    "compile": "gulp build",
    "build": "gulp build",
    "artifact": "NODE_ENV=prod gulp artifact",
    "predeploy": "npm run compile && npm run sync",
    "deploy": "NODE_ENV=prod NODE_APP_INSTANCE=lambda gulp deploy",
    "sync": "aws s3 sync ./config s3://etl-code-config/$npm_package_name/$npm_package_version/config"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/placester/Data-Services-ETL.git"
  },
  "author": "Placester, Inc",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.0.36",
    "@types/bunyan": "^0.0.36",
    "@types/chai": "^3.4.34",
    "@types/config": "0.0.31",
    "@types/faker": "^3.1.32",
    "@types/lodash": "^4.14.52",
    "@types/mocha": "^2.2.38",
    "@types/mongodb": "^2.1.37",
    "@types/node": "^6.0.45",
    "@types/sinon": "^1.16.31",
    "@types/sinon-as-promised": "^4.0.5",
    "@types/sinon-chai": "^2.7.27",
    "aws-sdk": "^2.9.0",
    "chai": "^3.5.0",
    "js-yaml": "^3.7.0",
    "node-aws-lambda": "^0.1.8",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "bunyan": "^1.8.1",
    "config": "^1.25.0",
    "infrastructure-logging-lib": "git+ssh://git@github.com:Placester/Infrastructure-Logging-Lib.git#2.1.1",
    "infrastructure-node-cloudservices-lib": "git+ssh://git@github.com:Placester/Infrastructure-Node-CloudServices-Lib.git#0.0.7",
    "infrastructure-node-dbutils": "git+ssh://git@github.com:Placester/Infrastructure-Node-DbUtils.git#0.0.2",
    "infrastructure-nodeaspect-lib": "git+ssh://git@github.com:Placester/Infrastructure-NodeAspect-Lib.git#1.0.2",
    "lodash": "^4.16.2",
    "moment": "^2.17.0",
    "mongodb": "^2.2.22"
  }
}
