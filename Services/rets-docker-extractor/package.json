{
  "name": "rets-docker-extractor",
  "version": "0.1.0",
  "private": true,
  "description": "Node.js app on Docker with some Amazon SQS and RETS",
  "author": "Placester Engineering <engineering@placester.com> (http://www.placester.com)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "gulp watch",
    "start": "node dist/index.js",
    "test": "gulp test",
    "compile": "gulp build",
    "predockerbuild": "npm run compile",
    "dockerbuild": "docker-compose build extractor",
    "dockerrun": "docker-compose run extractor",
    "itestImports": "NODE_ENV=test DEBUG=logginglib mocha -r tests/testpaths.js tests/integration/retsImporterService.integration.test.js",
    "itestContainer": "NODE_ENV=test DEBUG=logginglib mocha -r tests/testpaths.js tests/integration/container.test.js",
    "itestProcessor": "NODE_ENV=test mocha -r tests/testpaths.js tests/integration/processor.integration.test.js",
    "pushconfigs": "aws s3 sync ./config s3://etl-code-config/rets-docker-extractor/$npm_package_version/config",
    "pullconfigs": "aws s3 sync s3://etl-code-config/rets-docker-extractor/$npm_package_version/config ./config"
  },
  "dependencies": {
    "async": "^2.1.4",
    "aws-sdk": "^2.9.0",
    "bluebird": "^3.4.7",
    "bunyan": "^1.8.5",
    "chalk": "^1.1.3",
    "config": "^1.25.0",
    "debug": "^2.6.0",
    "infrastructure-logging-lib": "git+ssh://git@github.com:Placester/Infrastructure-Logging-Lib.git#2.1.1",
    "infrastructure-node-cloudservices-lib": "git+ssh://git@github.com:Placester/Infrastructure-Node-CloudServices-Lib.git#0.0.7",
    "infrastructure-nodeaspect-lib": "git+ssh://git@github.com:Placester/Infrastructure-NodeAspect-Lib.git#1.0.2",
    "inversify": "^3.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "reflect-metadata": "^0.1.9",
    "rets-client": "^5.0.1",
    "rx": "https://registry.npmjs.org/rx/-/rx-4.1.0.tgz",
    "rx-node": "^1.0.2",
    "shortid": "^2.2.6",
    "tslib": "^1.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.38",
    "@types/bluebird": "^3.0.37",
    "@types/bunyan": "0.0.35",
    "@types/config": "0.0.31",
    "@types/lodash": "^4.14.52",
    "@types/rx": "^4.1.1",
    "@types/shortid": "0.0.28",
    "app-module-path": "^2.1.0",
    "chai": "^3.5.0",
    "faker": "^3.1.0",
    "fs-extra": "^2.0.0",
    "js-yaml": "^3.7.0",
    "json2csv": "^3.7.3",
    "jsonfile": "^2.4.0",
    "minimist": "^1.2.0",
    "mongodb": "^2.2.22",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
